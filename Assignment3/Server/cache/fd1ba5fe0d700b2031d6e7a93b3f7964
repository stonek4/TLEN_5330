<div id="answer-19896305" class="answer" data-answerid="19896305"  itemscope itemtype="http://schema.org/Answer">
    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
        <input type="hidden" name="_id_" value="19896305">
        <a class="vote-up-off" title="This answer is useful">up vote</a>
